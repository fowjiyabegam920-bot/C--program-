#include <stdio.h>

// Function to calculate area
float calculateArea(float length, float breadth) {
    return length * breadth;
}

int main() {
    float l, b, area;

    printf("Enter length: ");
    scanf("%f", &l);

    printf("Enter breadth: ");
    scanf("%f", &b);

    area = calculateArea(l, b);

    printf("Area of rectangle = %.2f\n", area);

    return 0;
}
