#include <stdio.h>

int main() {
    int marks[5], i, j, temp;

    printf("Enter marks of 5 students:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
    }

    // Sorting using simple bubble sort
    for (i = 0; i < 4; i++) {
        for (j = i + 1; j < 5; j++) {
            if (marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("\nMarks in ascending order:\n");
    for (i = 0; i < 5; i++) {
        printf("%d ", marks[i]);
    }
    printf("\n");

    return 0;
}
