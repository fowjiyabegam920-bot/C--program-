#include <stdio.h>

int main() {
    int marks[5], i, topper;

    printf("Enter marks of 5 students:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
    }

    topper = marks[0];  // assume first student is topper

    for (i = 1; i < 5; i++) {
        if (marks[i] > topper) {
            topper = marks[i];  // update if higher marks found
        }
    }

    printf("Highest marks (Topper) = %d\n", topper);

    return 0;
}
