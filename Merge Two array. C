#include <stdio.h>

int main() {
    int arr1[5], arr2[5], merged[10];
    int i, j;

    // Input first array
    printf("Enter 5 elements of first array:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &arr1[i]);
        merged[i] = arr1[i];  // add to merged array
    }

    // Input second array
    printf("Enter 5 elements of second array:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &arr2[i]);
        merged[i + 5] = arr2[i];  // add to merged array
    }

    // Display merged array
    printf("\nMerged array:\n");
    for (i = 0; i < 10; i++) {
        printf("%d ", merged[i]);
    }
    printf("\n");

    return 0;
}
