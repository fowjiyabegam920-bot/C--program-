#include <stdio.h>
#include <string.h>

int main() {
    char items[5][20];
    float prices[5];
    char search[20];
    int i, found = 0;

    // Input items and their prices
    printf("Enter 5 items and their prices:\n");
    for (i = 0; i < 5; i++) {
        printf("Item %d name: ", i + 1);
        scanf("%s", items[i]);
        printf("Item %d price: ", i + 1);
        scanf("%f", &prices[i]);
    }

    // Search for an item
    printf("\nEnter item name to search: ");
    scanf("%s", search);

    for (i = 0; i < 5; i++) {
        if (strcmp(items[i], search) == 0) {
            printf("Price of %s = %.2f\n", items[i], prices[i]);
            found = 1;
            break;
        }
    }

    if (!found) {
        printf("Item not found!\n");
    }

    return 0;
}
